<launch>
    <!--
     This is the main node for sensor fusion, currently we have it set to
     ukf(quality data), but ekf(faster) is also an option. I have not tested the
     differences or at all attempted to compare the two, I just made a guess
     //TODO
    -->
    <node name="sensor_fusion" pkg="robot_localization"
      type="ukf_localization_node"  clear_params="true" output="screen">
        <rosparam command="load" file="$(find tfr_sensor)/params/fusion.yaml" />
    </node>
</launch>
